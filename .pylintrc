[MESSAGES CONTROL]
disable = missing-docstring, locally-disabled, too-few-public-methods, fixme, bad-continuation, import-error, no-member
msg-template = {module}:{line} [{msg_id}: {symbol}] - {msg}

[REPORTS]
reports = no

[FORMAT]
max-line-length = 120

[SIMILARITIES]
min-similarity-lines=6
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes
