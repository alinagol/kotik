[MESSAGES CONTROL]
disable = missing-docstring, locally-disabled, too-few-public-methods, fixme, bad-continuation
msg-template = {module}:{line} [{msg_id}: {symbol}] - {msg}

[REPORTS]
reports = no

[FORMAT]
max-line-length = 79

[SIMILARITIES]
min-similarity-lines=10
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes
